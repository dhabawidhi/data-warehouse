@model IEnumerable<DW_IPB.Models.Sarjana3>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.JumlahMahasiswa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Departeman.DepartemenNama)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JalurMasuk.JalurMasukNama)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JenisKelamin.JenisKelaminNama)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StatusAkademik.StatusAkademikNama)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WaktuTahunSarjana.TahunAkademik)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.JumlahMahasiswa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Departeman.DepartemenNama)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JalurMasuk.JalurMasukNama)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JenisKelamin.JenisKelaminNama)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StatusAkademik.StatusAkademikNama)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WaktuTahunSarjana.TahunAkademik)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
